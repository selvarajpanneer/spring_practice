<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="answer1" class="simapdependentobj.Answer">
<property name="id" value="1234"></property>
<property name="answer" value="java is programming language"></property>
<property name="postedDate"  value="20/1/2019"></property>
</bean>
<bean id="answer2" class="simapdependentobj.Answer">
<property name="id" value="1244"></property>
<property name="answer" value="java is island"></property>
<property name="postedDate"  value="20/1/2019"></property>
</bean>
<bean id="user1" class="simapdependentobj.User">  
<property name="id" value="1"></property>  
<property name="name" value="Arunr"></property>  
<property name="email" value="arun@gmail.com"></property>  
</bean>  
<bean id="user2" class="simapdependentobj.User">  
<property name="id" value="2"></property>  
<property name="name" value="Varun Kumar"></property>  
<property name="email" value="Varun@gmail.com"></property>  
</bean>  
  
<bean id="q" class="simapdependentobj.Question">  
<property name="id" value="1"></property>  
<property name="name" value="What is Java?"></property>  
<property name="answers">  
<map>  
<entry key-ref="answer1" value-ref="user1"></entry>  
<entry key-ref="answer2" value-ref="user2"></entry>  
</map>  
</property>  
</bean>  
</beans>